        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"$compose_service\", container=~\"$container\"}[$__rate_interval])) by (container)",
          "legendFormat": "{{container}}"
