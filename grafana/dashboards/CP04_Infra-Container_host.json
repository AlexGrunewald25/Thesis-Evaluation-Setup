      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0,
            "showPoints": "never",
            "stacking": { "mode": "none", "group": "A" },
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["lastNotNull", "mean", "max"]
