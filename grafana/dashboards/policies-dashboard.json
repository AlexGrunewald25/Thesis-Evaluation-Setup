      },
      {
        "id": 2,
        "type": "timeseries",
        "title": "REST latency (p50 / p95 / p99) by method",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 1 },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 20
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "targets": [
          {
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "expr": "histogram_quantile(0.5, sum(rate(policies_rest_latency_seconds_bucket[5m])) by (le, method))",
            "legendFormat": "p50 {{method}}"
          },
