{
  "id": null,
  "uid": "claims-service-research",
  "title": "Claims Service – Research KPIs",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "tags": [
    "claims",
    "research",
    "thesis"
  ],
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Key KPIs – submitClaim (by channel)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 2,
      "type": "stat",
      "title": "p95 latency – submitClaim by channel (s)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 2, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(claims_service_operation_duration_seconds_bucket{operation=\"submit\"}[5m])) by (le, channel))",
          "legendFormat": "p95 {{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "p99 latency – submitClaim by channel (s)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 2, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum(rate(claims_service_operation_duration_seconds_bucket{operation=\"submit\"}[5m])) by (le, channel))",
          "legendFormat": "p99 {{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Throughput – submitClaim success (ops/s) by channel",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 7 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops/s",
          "decimals": 3,
          "min": 0,
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(claims_service_operation_total{operation=\"submit\"}[1m])) by (channel)",
          "legendFormat": "{{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Error rate – submitClaim (ops/s) by channel",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 7 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops/s",
          "decimals": 3,
          "min": 0,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.01, "color": "yellow" },
              { "value": 0.05, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(claims_service_operation_total{operation=\"submit\", outcome!=\"success\"}[1m])) by (channel, outcome)",
          "legendFormat": "{{channel}} ({{outcome}})",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },

    {
      "id": 6,
      "type": "row",
      "title": "Latency curves – submitClaim (p50 / p95 / p99)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "submitClaim latency by channel (p50 / p95 / p99)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum(rate(claims_service_operation_duration_seconds_bucket{operation=\"submit\"}[5m])) by (le, channel))",
          "legendFormat": "p50 {{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum(rate(claims_service_operation_duration_seconds_bucket{operation=\"submit\"}[5m])) by (le, channel))",
          "legendFormat": "p95 {{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum(rate(claims_service_operation_duration_seconds_bucket{operation=\"submit\"}[5m])) by (le, channel))",
          "legendFormat": "p99 {{channel}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },

    {
      "id": 8,
      "type": "row",
      "title": "Resource Utilization – Claims Service JVM",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "CPU usage (system & process)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "system_cpu_usage * 100",
          "legendFormat": "system_cpu_usage",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "refId": "B",
          "expr": "process_cpu_usage * 100",
          "legendFormat": "process_cpu_usage",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Heap memory used",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(jvm_memory_used_bytes{area=\"heap\"})",
          "legendFormat": "heap used",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true
      }
    ]
  }
}
